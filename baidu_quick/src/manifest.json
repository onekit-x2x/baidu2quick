{
  "package": "com.example.demo",
  "name": "baidu2quick",
  "versionName": "1.0.0",
  "versionCode": "1",
  "icon":"onekit/logo.png",
  "minPlatformVersion": "1069",
  "features": [
    {
      "name": "system.prompt"
    },
    {
      "name": "system.router"
    },
    {
      "name": "system.shortcut"
    }
  ],
  "permissions": [
    {
      "origin": "*"
    }
  ],
  "config": {
    "logLevel": "off"
  },
  "router": {
    "entry": "Demo",
    "pages": {
      "Demo": {
        "component": "index"
      },
      "Test":{
        "component": "index",
        "path":"/test/index"
      }

    }
  },
  "display": {
    "titleBarBackgroundColor": "#f2f2f2",
    "titleBarTextColor": "#414141",
    "menu": true,
    "pages": {
      "Demo": {
        "titleBarText": "示例页",
        "menu": false
      }
    }
  }
}
